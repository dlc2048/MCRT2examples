ROT_DEFI name=rook_rot  axis=x theta=-90 delta=-5,-3,0
ROT_DEFI name=rook_0    axis=z theta=0
ROT_DEFI name=rook_120  axis=z theta=120
ROT_DEFI name=rook_240  axis=z theta=240
ROT_DEFI name=queen_rot axis=x theta=-90 delta=5,-3,0
ROT_DEFI name=queen_0   axis=z theta=0
ROT_DEFI name=queen_120 axis=z theta=120
ROT_DEFI name=queen_240 axis=z theta=240
ROT_DEFI name=king_rot  axis=x theta=-90 delta=10,-3,0

SURFACE_BEGIN
    # pawn
    REVOLUTION name=pawn entry=pawn_co center=-10,-3,0 direction=0,1,0
    # rook
    TRANSFORM_BEGIN target=rook_rot
        REVOLUTION name=rook center=0,0,0 entry=rook_co 
        TRANSFORM_BEGIN target=rook_0
            CUBE name=rook_h0   center=0,0,5.6 size=0.4,10,0.6
        TRANSFORM_END
        TRANSFORM_BEGIN target=rook_120
            CUBE name=rook_h120 center=0,0,5.6 size=0.4,10,0.6
        TRANSFORM_END
        TRANSFORM_BEGIN target=rook_240
            CUBE name=rook_h240 center=0,0,5.6 size=0.4,10,0.6
        TRANSFORM_END
    TRANSFORM_END
    # bishop
    REVOLUTION name=bishop entry=bishop_co center=0,-3,0   direction=0,1,0
    # queen
    TRANSFORM_BEGIN target=queen_rot
        REVOLUTION name=queen    center=0,0,0     entry=queen_co
        UVSPHERE   name=queen_b1 center=0,0,7.15  radius=0.5
        UVSPHERE   name=queen_b2 center=0,0,7.8   radius=0.3
        TRANSFORM_BEGIN target=queen_0
            CYLINDER name=queen_h0   center=0,-5,7.5 radius=0.34 height=0,10,0
        TRANSFORM_END
        TRANSFORM_BEGIN target=queen_120
            CYLINDER name=queen_h120 center=0,-5,7.5 radius=0.34 height=0,10,0
        TRANSFORM_END
        TRANSFORM_BEGIN target=queen_240
            CYLINDER name=queen_h240 center=0,-5,7.5 radius=0.34 height=0,10,0
        TRANSFORM_END
    TRANSFORM_END
    # king
    TRANSFORM_BEGIN target=king_rot
        REVOLUTION name=king    center=0,0,0    entry=king_co   
        CUBE       name=king_c1 center=0,0,8.65 size=0.3,0.15,1.4
        CUBE       name=king_c2 center=0,0,8.8  size=1.0,0.15,0.3
    TRANSFORM_END
SURFACE_END

REGION pawn    pawn
REGION rook    rook & -(rook_h0 | rook_h120 | rook_h240)
REGION bishop  bishop
REGION queen   (queen & -(queen_h0 | queen_h120 | queen_h240)) | queen_b1 | queen_b2
REGION king    king | king_c1 | king_c2
REGION void    -@pawn & -@rook & -@bishop & -@queen & -@king